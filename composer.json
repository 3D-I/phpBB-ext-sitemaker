    "type": "phpbb-extension",
	"description": "A CMS for phpBB3.1x.",
	"homepage": "https://github.com/blitze/primetime",
	"version": "2.0.1-dev",
	"time": "2014-01-14 01:01:01",
	"license": "GPL-2.0",
	"authors": [{
			"name": "Daniel A.",
			"email": "shizzah@hotmail.com",
			"homepage": "https://github.com/blitze",
			"role": "Lead Developer"
		}],
	"require": {
		"urodoz/truncate-html" : "@stable",
		"composer/installers": "~1.0",
		"php": ">=5.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master",
		"codeclimate/php-test-reporter": "dev-master"
	},
	"extra": {
		"display-name": "phpBB Primetime",
